{
  "name": "code-ai",
  "displayName": "code.ai",
  "description": "code assistant",
  "version": "0.0.1",
  "publisher": "bornbrave",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codeai.activateSpeechRecognition"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "codeai.activateSpeechRecognition",
        "title": "Hey code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "tt": "mocha --require ts-node/register src/test-ai/*.ts --watch --watch-extensions ts"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^7.10.0",
    "ts-node": "^7.0.1",
    "typescript": "^2.9.2",
    "vscode": "^1.1.21",
    "vscode-languageclient": "^3.5.1",
    "vscode-languageserver": "^3.5.1"
  },
  "dependencies": {
    "express": "^4.16.4",
    "opn": "^5.4.0",
    "ws": "^3.3.3"
  }
}
